import java.io.*;
public class Solution 
{
    public static void main(String[] args)throws IOException
     {
         DataInputStream d=new DataInputStream(System.in);
        int N = Integer.parseInt(d.readLine());
        for(int i=1;i<=10;i++)
        {
            System.out.println(N+" x "+i+" = "+N*i);
        }
    }
}
